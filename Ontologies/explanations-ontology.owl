    <!-- http://www.semanticweb.org/shruthichari/ontologies/2020/3/explanation-ontology#addresses -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shruthichari/ontologies/2020/3/explanation-ontology#addresses">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000294"/>
        <dc:description>a addresses b if a provides an answer to b</dc:description>
        <rdfs:label>addresses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shruthichari/ontologies/2020/3/explanation-ontology#asks -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shruthichari/ontologies/2020/3/explanation-ontology#asks">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000365"/>
