    <!-- http://www.semanticweb.org/shruthichari/ontologies/2020/3/explanation-ontology#User_Context -->

    <owl:Class rdf:about="http://www.semanticweb.org/shruthichari/ontologies/2020/3/explanation-ontology#User_Context">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shruthichari/ontologies/2020/3/explanation-ontology#Contextual_Knowledge"/>
        <rdfs:subClassOf>
